{
    "name": "phpunit/test-example",
    "type": "example",
    "description": "A simple example on phpunit testing.",
    "keywords": [
        "php",
        "unit",
        "test",
        "simple",
        "example"
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
